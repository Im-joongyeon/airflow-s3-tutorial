# Airflow Settings
AIRFLOW_UID=50000
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=airflow

# AWS Credentials for S3 DAG Sync
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=ap-northeast-2

# S3 DAG Configuration
S3_DAG_BUCKET=your-bucket-name
S3_DAG_PREFIX=dags/
SYNC_INTERVAL=300

# RDS Configuration (for RDS DAG examples)
RDS_HOST=your-rds-endpoint.region.rds.amazonaws.com
RDS_PORT=3306
RDS_DATABASE=your_database_name
RDS_SECRET_NAME=your-secrets-manager-secret-name
